{
  "name": "ship-yard",
  "version": "1.0.0",
  "description": "A build and execution environment for React/Redux apps",
  "main": "lib/index.js",
  "scripts": {
    "test": "istanbul test _mocha -- --require test/setup test/**/*.{js,jsx}",
    "build": "babel src -s -d lib",
    "watch": "babel src -s -d lib -w",
    "bundle": "babel-node $(which webpack) --display-modules"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/ship-yard.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "build",
    "develop",
    "universal"
  ],
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^4.1.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-export-extensions": "^6.1.18",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015-loose": "^6.1.2",
    "babel-register": "^6.2.0",
    "chai": "^3.4.1",
    "eslint": "^1.9.0",
    "eslint-config-ecliptic": "^1.3.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.3",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.9"
  }
}
