{
  "name": "mission-ctrl",
  "version": "1.0.0-dev.1",
  "description": "A build and execution environment for React/Redux apps",
  "main": "lib/index.js",
  "bin": {
    "ship": "./bin/ship"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/mission-ctrl.git"
  },
  "scripts": {
    "build": "NODE_PATH=$NODE_PATH:src NODE_ENV=development babel src -s -d lib --copy-files",
    "build:watch": "npm run build -- -w",
    "parallel": "parallel --no-notice --line-buffer --halt now,fail=1 :::",
    "start": "npm run parallel -- 'npm run build:watch' 'npm run test:watch'",
    "test": "NODE_PATH=$NODE_PATH:lib NODE_ENV=test istanbul test _mocha -- --reporter min --require utils/test-setup lib/**/*.test.js",
    "test:watch": "chokidar -t 300 --silent 'lib/**/*.js' -c 'npm test'"
  },
  "author": "Brandon Konkle <brandon@konkle.us>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "build",
    "develop",
    "universal"
  ],
  "dependencies": {
    "app-module-path": "^1.0.5",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-polyfill": "^6.3.14",
    "blessed": "^0.1.81",
    "bunyan": "^1.5.1",
    "chalk": "^1.1.1",
    "chokidar": "^1.4.2",
    "eslint": "^1.10.3",
    "findup-sync": "^0.3.0",
    "glob": "^7.0.0",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.1",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "ramda": "^0.19.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.5.0",
    "redux-saga": "^0.7.0",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.1",
    "slug": "^0.9.1",
    "source-map-support": "^0.4.0",
    "through2": "^2.0.1",
    "uuid": "^2.0.1",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.5.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-register": "^6.5.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chai-immutable": "^1.5.3",
    "chokidar-cli": "^1.2.0",
    "eslint-config-ecliptic": "^1.3.0",
    "proxyquire": "^1.7.4",
    "redux-mock-store": "0.0.6",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "terminus": "^1.1.0"
  }
}
