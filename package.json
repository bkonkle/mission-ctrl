{
  "name": "ship-yard",
  "version": "1.0.0",
  "description": "A build and execution environment for React/Redux apps",
  "main": "lib/index.js",
  "scripts": {
    "build": "NODE_PATH=$NODE_PATH:src NODE_ENV=development babel src -s -d lib",
    "test": "NODE_PATH=$NODE_PATH:src NODE_ENV=test istanbul test _mocha -- --require test/setup test/**/*.js",
    "watch": "npm run build -- -w",
    "test-watch": "npm run test -- -w"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ecliptic/ship-yard.git"
  },
  "author": "Brandon Konkle <brandon@konkle.us>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "build",
    "develop",
    "universal"
  ],
  "dependencies": {
    "babel-core": "^6.4.5",
    "bunyan": "^1.5.1",
    "chalk": "^1.1.1",
    "findup-sync": "^0.3.0",
    "glob": "^6.0.4",
    "immutable": "^3.7.6",
    "minimist": "^1.2.0",
    "ramda": "^0.19.1",
    "rc": "^1.1.6",
    "redux-thunk": "^1.0.3",
    "webworker-threads": "^0.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-class-properties": "^6.2.2",
    "babel-plugin-transform-export-extensions": "^6.1.18",
    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "eslint": "^1.9.0",
    "eslint-config-ecliptic": "^1.3.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.3",
    "proxyquire": "^1.7.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.9.1",
    "redux-mock-store": "0.0.6",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.9"
  }
}
